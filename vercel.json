{
    "version": 2,
    "builds": [
      {
        "src": "api/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/api/update-feed", "dest": "/api/update-feed" },
      { "src": "/api/fscollegian", "dest": "/api/fscollegian" },
      { "src": "/(.*)", "dest": "/public/$1" }
    ]
  }
  